{
  "name": "Tabber",
  "version": "1.0",
  "content_scripts": [
    {
        "matches": [
            "<all_urls>"
        ],
        "js": ["popup.js"]
    }
],
  "permissions": [
      "activeTab", "tabs"
    ],
  "browser_action": {
    "default_popup": "popup.html"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "icons": {"128": "logo.png" },
  "manifest_version": 2
}
